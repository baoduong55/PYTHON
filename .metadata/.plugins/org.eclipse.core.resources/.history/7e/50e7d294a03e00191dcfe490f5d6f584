import numpy as np
A = np.random.choice(100, 10, replace = False)
print(A)
def Merge(X,Y):
    result = []
    for x in range(len(X)):
        for y in range(len(Y)):
            if X[x] < Y[y]:
                result.append(X[x])
                x += 1
            else
                result.append(Y[y])
                 
            if x == range(len(X))-1:
                result.append(Y[y])
                y = range(len(Y))-1
            else if y == range(len(Y))-1:
                result.append(X[x])
                break
    return        
